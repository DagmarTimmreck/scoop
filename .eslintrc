{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-unused-expressions": 0,
    "quote-props": 0,
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "dot-notation": 0,
    "import/newline-after-import": 0,
    "max-len": 0,
    "no-plusplus": 0,
    "consistent-return": 0,
    "no-console": 0,
    "one-var": 0,
    "prefer-const": 0,
  }
}
